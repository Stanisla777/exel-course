{
  "parser":"babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "space-before-blocks": "off",
    "no-console": "off",
    "require-jsdoc": "off",
    "padded-blocks": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
